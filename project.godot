DebugCore="*res://addons/debug_console/core/DebugCore.gd"
GameConsoleManager="*res://addons/debug_console/game/GameConsoleManager.gd"

[debug]

enable_game_console=true

[editor_plugins]

enabled=PackedStringArray("res://addons/debug_console/plugin.cfg")
